{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/auth/login",
            "dest": "/index.js" 
        },
        {
            "src": "/auth/register",
            "dest": "/index.js" 
        },
        {
            "src": "/auth/reset-password",
            "dest": "/index.js" 
        },
        {
            "src": "/equipos",
            "dest": "/index.js" 
        },
        {
            "src": "/novedades/todas",
            "dest": "/index.js" 
        },
        {
            "src": "/novedades/hoy",
            "dest": "/index.js" 
        },
        {
            "src": "/novedades",
            "dest": "/index.js" 
        },
        {
            "src": "/(.*)", 
            "dest": "/"
        }
    ]
}